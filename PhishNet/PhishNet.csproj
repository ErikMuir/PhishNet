<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <PackageId>PhishNet</PackageId>
    <Version>0.1.0</Version>
    <Authors>Erik Muir</Authors>
    <Description>A C#/.NET wrapper around the Phish.net API</Description>
    <Copyright>Copyright 2024 (c) Erik Muir. All rights reserved.</Copyright>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/ErikMuir/PhishNet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ErikMuir/PhishNet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="8.0.11" />
  </ItemGroup>

</Project>
